{
  "expo": {
    "name": "Prime",
    "slug": "Prime",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      
      "resizeMode": "contain",
      "backgroundColor": "#232F3E"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        
        "backgroundColor": "#232F3E"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
